<!DOCTYPE html>
<html lang="en">
  <head>
    <% include ../tmpl/head %>
  </head>
  <body class="container">
    <header>
      <% include ../tmpl/header %>
    </header>

    <main>
      <div class="page-header">
        <h2>Statistics</h2>
      </div>
      <form id="form-filter" method="post" action="/filter">
        <div class="form form-horizontal">
          <div class="form-group">
            <label class="col-sm-2">Earliest</label>
            <div class="input-append date form-date">
              <input size="10" type="text" name="earliest" value=""2015-12-31"" readonly>
              <span class="add-on"><i class="icon-th"></i></span>
            </div>
          </div>
          <div class="form-group">
            <label class="col-sm-2">Latest</label>
            <div class="input-append date form-date">
              <input size="10" type="text" name="earliest" value=""2015-12-31"" readonly>
              <span class="add-on"><i class="icon-th"></i></span>
            </div>
          </div>
        </div>
        <div class="form-group">
          <div class="col-sm-offset-2 col-sm-8">
            <input class="btn btn-primary" type="submit" />
          </div>
        </div>
      </form>
    </main>

    <footer>
      <% include ../tmpl/footer %>
    </footer>
  </body>
</html>
